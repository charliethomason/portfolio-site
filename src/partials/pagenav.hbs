  <li class="next"><a href="{{next.id}}.html"><img src="../img/{{#if folder}}{{folder}}{{else}}{{../../folder}}{{/if}}/thumbs/{{next.id}}-250.jpg" alt="{{next.title}}"><span>{{next.title}}</span></a></li>
  <li class="back"><a href="../{{#if folder}}{{folder}}{{else}}{{../../folder}}{{/if}}.html"><img src="../img/pagenav-gray.jpg" alt=""><span>Return to {{#if folder}}{{capitalizeFirst folder}}{{else}}{{capitalizeFirst ../../folder}}{{/if}}</span></a></li>
</nav>
{{/pagenav}}